			Ajax.call(
				'plugins/kuaidi100/kuaidi100_post.php?com=' + expressid + '&nu=' + expressno,
				'showtest=showtest',
				function(data) {
					document.getElementById("retData").innerHTML = '快递公司：' + expressid + ' &nbsp; 运单号：' + expressno + '<br>';
					document.getElementById("retData").innerHTML += data;
				},
				'GET',
				'TEXT',
				false
			);
		}

